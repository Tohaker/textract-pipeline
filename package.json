{
	"name": "textract-pipeline",
	"version": "1.0.0",
	"description": "Pipeline infrastructure and application for the AWS Textract service",
	"type": "module",
	"scripts": {
		"deploy": "cdk deploy --all --require-approval never",
		"check:types": "tsc",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"test": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Tohaker/textract-pipeline.git"
	},
	"author": "Miles Bardon",
	"license": "UNLICENSED",
	"bugs": {
		"url": "https://github.com/Tohaker/textract-pipeline/issues"
	},
	"homepage": "https://github.com/Tohaker/textract-pipeline#readme",
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@tsconfig/node22": "22.0.5",
		"@types/aws-lambda": "8.10.160",
		"@types/node": "25.0.10",
		"aws-cdk": "2.1103.0",
		"aws-sdk-client-mock": "4.1.0",
		"aws-sdk-client-mock-jest": "4.1.0",
		"chalk": "5.6.2",
		"typescript": "5.9.3",
		"vitest": "4.0.18"
	},
	"dependencies": {
		"@aws-lambda-powertools/batch": "2.30.2",
		"@aws-lambda-powertools/jmespath": "2.30.2",
		"@aws-lambda-powertools/logger": "2.30.2",
		"@aws-sdk/client-s3": "3.975.0",
		"@aws-sdk/client-textract": "3.975.0",
		"amazon-textract-response-parser": "0.4.3",
		"aws-cdk-lib": "2.236.0",
		"aws-lambda": "1.0.7",
		"constructs": "10.4.5",
		"source-map-support": "0.5.21"
	}
}
